{
  "manifest_version": 2,
  "name": "Github Tools",
  "version": "0.1.0",
  "description": "Some simple tweaks to Github",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": [
        "styles.css",
        "css/pr-java.css"
      ],
      "js": [
        "js/lib/jquery-2.1.1.min.js",
        "js/content-scripts/pr-java.js",
        "js/content-scripts/wide-mode.js"
      ]
    }
  ]
}
